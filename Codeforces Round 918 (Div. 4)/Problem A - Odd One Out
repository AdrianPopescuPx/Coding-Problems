Problem Link https://codeforces.com/contest/1915/problem/A

#include <cmath>
#include <iomanip>
#include <iostream>
using namespace std;
 
 
int main() {
    
    int t, a, b, c;
    cin >> t;
    while(t--) {
        cin >> a >> b >> c;
        if (a == b) {
            cout << c;
        }   else if (a == c) {
            cout << b;
        }   else cout << a;
        cout << '\n';
    }
   
 
    return 0;
}
